{
  "name": "@oramacloud/vue-client",
  "version": "2.0.1",
  "description": "Orama SDK for Vue",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "runkitExampleFilename": "./example/runkit.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --config tsup.vue.js --watch",
    "build": "tsup --config tsup.vue.js"
  },
  "keywords": [
    "orama",
    "search engine",
    "sdk"
  ],
  "author": {
    "name": "Michele Riva",
    "email": "michele.riva@oramasearch.com",
    "url": "https://github.com/MicheleRiva"
  },
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.21",
    "@oramacloud/client": "workspace:*",
    "@orama/orama": "^2.0.16",
    "vue": "^3.4.25"
  },
  "devDependencies": {
    "ts-standard": "^12.0.2",
    "@types/lodash": "^4.14.202",
    "tsup": "^8.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "ts-standard": {
    "ignore": [
      "dist",
      "node_modules"
    ]
  }
}