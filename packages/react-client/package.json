{
  "name": "@oramacloud/react-client",
  "version": "2.0.1",
  "description": "Orama SDK for React",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --config tsup.react.js --watch",
    "build": "tsup --config tsup.react.js"
  },
  "keywords": [
    "orama",
    "react",
    "search engine",
    "sdk"
  ],
  "files": [
    "dist",
    "example/runkit.js"
  ],
  "author": {
    "name": "Michele Riva",
    "email": "michele.riva@oramasearch.com",
    "url": "https://github.com/MicheleRiva"
  },
  "license": "ISC",
  "dependencies": {
    "@orama/orama": "^2.0.16",
    "react": "^18.2.0",
    "@oramacloud/client": "workspace:*",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/lodash": "^4.14.202",
    "dotenv": "^16.3.1",
    "ts-standard": "^12.0.2",
    "tsup": "^8.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "ts-standard": {
    "ignore": [
      "dist",
      "node_modules"
    ]
  }
}